package root;

import root.Personne;

public class Program {
	public static void main(String[] args)
	{
		System.out.println("-- Program Start --");
		
		Personne hPersonne1 = new Personne("GUIRAUD", "Gwenaël", 23, 5, 2000, 43, "Pascal Paoli", "20620", "Biguglia");
		Personne hPersonne2 = new Personne("GUIRAUD", "Gwenaël", 23, 5, 2000, 43, "Pascal Paoli", "20620", "Biguglia");
		Personne hPersonne3 = new Personne("GUIRAUD", "Maëlle", 18, 12, 2002, 43, "Pascal Paoli", "20620", "Biguglia");
		
		System.out.println("Nombre d'instance de personne: " + Personne.getNbrInstances());
		System.out.println("hPersonne1 == hPersonne2: " + hPersonne1.equals(hPersonne2));
		System.out.println("hPersonne1 == hPersonne3: " + hPersonne1.equals(hPersonne3));
		
		System.out.println("hPersonne1 plus agee que hPersonne3: " + hPersonne1.plusAgeeQue(hPersonne3));
		
		System.out.println("-- Program Start --");
	}
}
